prefix=@CMAKE_INSTALL_PREFIX@
exec_prefix=${prefix}
libdir=${prefix}/lib
includedir=${prefix}/include

Name: yukkipaste
Description: Modular pastebin file uploader
URL: http://github.com/eientei/yukkipaste
Version: 1.1
Cflags: -I${includedir}/yukkipaste
Libs: -L${libdir}
